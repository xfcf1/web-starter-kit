{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "semicolon": [true, "never"],
    "ordered-imports": false,
    "no-console": false,
    "trailing-comma": false,
    "member-access": false
  },
  "rulesDirectory": ["./src"]
}